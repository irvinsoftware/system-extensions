<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <RootNamespace>Irvin.Extensions</RootNamespace>
    <AssemblyName>SystemExtensions</AssemblyName>
    <Version>1.4.0</Version>
    <Product />
    <Authors>Nate Irvin</Authors>
    <Copyright>Copyright Â© Nathaniel Irvin 2013-2023</Copyright>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>Irvin.SystemExtensions</PackageId>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Description>A .NET Standard 2.1 library containing enhancements to System libraries.</Description>
    <RepositoryUrl>https://github.com/irvinsoftware/system-extensions</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageVersion>1.4.0.1</PackageVersion>
    <AssemblyVersion>1.4.0</AssemblyVersion>
    <FileVersion>1.4.0</FileVersion>
    <LangVersion>10</LangVersion>
    <PackageIcon>logo.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Update="logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
